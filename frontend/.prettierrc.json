{
  "arrowParens": "avoid",
  "singleQuote": true,
  "semi": false,
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "<BUILTIN_MODULES>",
    "^react",
    "<THIRD_PARTY_MODULES>",
    "^@[/]",
    "^[.].*$",
    "<TYPES>",
    "^[/].*$",
    "[.]css$"
  ],
  "endOfLine": "crlf"
}
